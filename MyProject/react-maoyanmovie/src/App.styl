.App
  width 100%
  height 100%
  color #000000
  background-color #c62f2f
  .header
    color: #ffd700
    height 50px
    line-height: 50px
    text-align: center
  .tabbar
    position: fixed
    bottom 0
    display: flex
    width 100%
    height 70px
    border-top: 1px solid #e8e8e8
    .tabbar-item
      flex: 1
      height 100%
      padding-top: 5px

.nav-link, .link
  display: block
  color:#000
  text-decoration: none
.active
  color: #FFD700
  transform: scale(1.15)

    